!!!
%html
  %head
    %meta{ :charset => "utf-8" }/
    %title Photoquest
    %meta{ :name => "keywords", :content => "" }/
    %meta{ :name => "description", :content => "" }/
    = stylesheet_link_tag "reset", "photoquest", :cache => "all", :media => "all"
    %link{ :rel => "stylesheet", :href => "http://fonts.googleapis.com/css?family=Vollkorn&subset=latin" }
    = javascript_include_tag "jquery.min.js", "rails.js", "application.js"
    = csrf_meta_tag
  %body
    #wrap
      #header
        %h1
          Photoquest - The challenge
        #logo
          = link_to image_tag("photoquest-logo.png", :alt => "Photoquest", :width => "387", :height => "107" ), "/"
      %hr
      #content
        = yield
      %hr
      #footer
        %p
          &copy; 2010 Photoquest
    = render :partial => "layouts/speedo" 
    = render :partial => "layouts/analytics" 
    %script{:type=>"text/javascript"}
      var rumbleUrl = 'http://railsrumble.com';
      %script{:type=>"text/javascript", :src=>"http://blog.railsrumble.com/javascripts/rumble-r10-sash.js"}
